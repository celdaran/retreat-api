    "type": "project",
    "description": "RESTful API for Retreat: a retirement planning tool",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-pdo": "*",
        "vlucas/phpdotenv": "^5.5",
        "restler/framework": "^5"
    },
    "require-dev": {
